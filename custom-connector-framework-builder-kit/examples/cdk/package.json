{
  "name": "custom-connector-framework-examples-cdk",
  "version": "0.1.0",
  "bin": {
    "cdk-app": "bin/cdk-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests",
    "cdk": "bash scripts/ensure-requirements.sh && cdk",
    "deploy": "bash scripts/ensure-requirements.sh && cdk deploy --all",
    "lint": "eslint --config eslint.config.mjs .",
    "lint:fix": "eslint --config eslint.config.mjs . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\" && cd lambdas && make format",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "clean": "rm -rf cdk.out node_modules && find . -type f \\( -name '*.d.ts' -o -name '*.js' \\) -not -path './node_modules/*' -not -name 'jest.config.js' -delete && cd lambdas && make clean",
    "py:install": "cd lambdas && make install",
    "py:format": "cd lambdas && make format",
    "py:validate": "cd lambdas && make validate",
    "py:test": "cd lambdas && make test"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.7",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "aws-cdk": "2.1019.2",
    "eslint": "9.30.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.2.0",
    "jest": "30.0.3",
    "prettier": "3.6.2",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.202.0",
    "constructs": "10.4.2",
    "source-map-support": "0.5.21"
  }
}
